{
  "name": "@therapy/jsx",
  "version": "0.0.2",
  "license": "MIT",
  "exports": {
    ".": "./mod.ts",
    "./types": "./types.d.ts",
    "./jsx": "./jsx.ts",
    "./jsx-types": "./jsx.d.ts"
  },
  "compilerOptions": {
    "lib": [
      "deno.ns",
      "dom",
      "dom.iterable"
